    {
      "id": "5d2e0dde61b5e742729b4ff5",
      "metadata": {
        "resourceId": "\"5d2e0dde61b5e742729b4ff6\"",
        "headingReferenceId": "\"H15\"",
        "contentId": "3464",
        "isOriginalDescription": "false",
        "active": "false",
        "text": "\"UpToDate content\"",
        "url": "\"http://editorial00p/Topic/OpenTopic.aspx?view=web&TopicID=3464&Bookmark=H15\"",
        "resourceType": "\"utd-MED\""
      },
      "tags": [

        "layout": "{\"width\":140,\"height\":80,\"offsetNodeByY\":0,\"offsetNodeByX\":0}",
        "isCustomSummary": "true",
        "isCollapsed": "false",
        "questionForChildren": "\"<p><strong>Has the patient's precipitating disorder (or disorders) been suitably treated?</strong></p>\\n\"",
        "variable": "\"suitable_rx\"",
        "shortTitle": "\"\"",
        "title": "\"<p>Has the precipitating</p>\\n\\n<p>factor (or factors) been</p>\\n\\n<p>suitably treated?</p>\\n\"",
        "resourceIds": "[]"
      },
      "tags": [
